{
    "name": "tquadra/phpwsdl2",
    "description": "PhpWSDL2 - Modern PHP WSDL/SOAP WebService Library with Multiple Protocol Support",
    "type": "library",
    "keywords": ["wsdl", "soap", "webservice", "json", "rest", "rpc", "http", "client", "server"],
    "homepage": "https://github.com/tquadra/phpwsdl2",
    "license": "MIT",
    "authors": [
        {
            "name": "tQuadra Team",
            "email": "info@tquadra.it",
            "homepage": "https://www.tquadra.it"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-soap": "*",
        "ext-json": "*",
        "ext-reflection": "*",
        "laminas/laminas-soap": "^2.10",
        "tecnickcom/tcpdf": "^6.4",
        "setasign/fpdi": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "PhpWSDL2\\": "src/PhpWSDL2/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpWSDL2\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse src --level=8",
        "cs-check": "phpcs src --standard=PSR12",
        "cs-fix": "phpcbf src --standard=PSR12"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
